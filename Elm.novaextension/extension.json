{
    "identifier": "sciencefidelity.elm",
    "name": "Elm",
    "organization": "Matt Cook",
    "description": "Elm Language Definition for Nova.",
    "version": "0.1.0",
    "categories": ["languages"],
    "license": "MIT",
    "repository": "https://github.com/sciencefidelity/Nova-Elm",
    "bugs": "https://github.com/sciencefidelity/Nova-Elm/issues",
    "homepage": "https://mattcook.dev",
    "keywords": ["elm"],
    "main": "main.dist.js",
    "activationEvents": [
        "onLanguage:elm",
        "onWorkspaceContains:elm.json"
    ],
    "entitlements": {
        "process": true,
        "filesystem": "readonly"
    },
    "config": [
        {
            "key": "sciencefidelity.elm.config.enableAnalyzer",
            "title": "Enable Language Server",
            "type": "boolean",
            "default": false
        }
    ]

}
